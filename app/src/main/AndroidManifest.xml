<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.dev.databindingexample">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.DataBindingImplementation" >

        <activity
            android:name="com.dev.databidingexample.TwoWayDataBindingActivity"
            android:exported="true" />
        <activity
            android:name="com.dev.databidingexample.OneWayDataBindingActivity"
            android:exported="true" />
        <activity
            android:name="com.dev.databidingexample.SimpleDataBindingActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

    </application>

</manifest>